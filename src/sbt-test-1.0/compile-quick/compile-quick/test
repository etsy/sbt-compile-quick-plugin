# Runs compileQuick
> compileQuick com/etsy/sbt/Main.scala
$ exists target/scala-2.12/classes/com/etsy/sbt/Main.class
# Runs compileQuick on a test
> test:compileQuick com/etsy/sbt/Test.scala
$ exists target/scala-2.12/test-classes/com/etsy/sbt/Test.class
# Runs packageQuick
> packageQuick
$ exists target/scala-2.12/compile-quick-test_2.12-0.1.jar
